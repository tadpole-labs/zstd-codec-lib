{
  "name": "zstd-wasm-decoder",
  "version": "0.2.1",
  "private": false,
  "type": "module",
  "description": "Tiny & performant decoder-only implementation of Zstandard",
  "keywords": [
    "zstd",
    "wasm",
    "decompression",
    "streaming",
    "dictionary",
    "DecompressionStreams API"
  ],
  "license": "(Apache-2.0 OR MIT) AND BSD-3-Clause",
  "bugs": "https://github.com/tadpole-labs/zstd-codec-lib/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tadpole-labs/zstd-codec-lib.git"
  },
  "main": "./_esm/index.node.js",
  "types": "./_types/index.d.ts",
  "typings": "./_types/index.d.ts",
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "node": {
        "types": "./_types/index.node.d.ts",
        "import": "./_esm/index.node.js",
        "default": "./_esm/index.node.js"
      },
      "browser": {
        "types": "./_types/index.web.inlined.d.ts",
        "import": "./_esm/index.inlined.js",
        "default": "./_esm/index.inlined.js"
      },
      "development": "./_esm/index.inlined.js",
      "import": "./_esm/index.inlined.js",
      "default": "./_esm/index.node.js"
    },
    "./external": {
      "types": "./_types/index.web.d.ts",
      "development": "./_esm/index.inlined.js",
      "import": "./_esm/index.web.js",
      "default": "./_esm/index.web.js"
    },
    "./cloudflare": {
      "types": "./_types/index.cloudflare.d.ts",
      "import": "./_esm/index.cloudflare.js",
      "default": "./_esm/index.cloudflare.js"
    },
    "./perf": {
      "types": "./_types/index.web.inlined.d.ts",
      "import": "./_esm/index.inlined.perf.js",
      "default": "./_esm/index.inlined.perf.js"
    },
    "./perf/external": {
      "types": "./_types/index.web.d.ts",
      "import": "./_esm/index.web.perf.js",
      "default": "./_esm/index.web.perf.js"
    },
    "./wasm": "./zstd-decoder.wasm",
    "./wasm-perf": "./zstd-decoder-perf.wasm",
    "./types": {
      "types": "./_types/index.d.ts",
      "default": "./_types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./_types/*"
      ],
      "external": [
        "./_types/index.web.d.ts"
      ],
      "cloudflare": [
        "./_types/index.cloudflare.d.ts"
      ],
      "perf": [
        "./_types/index.web.inlined.d.ts"
      ],
      "perf/external": [
        "./_types/index.web.d.ts"
      ],
      "types": [
        "./_types/index.d.ts"
      ]
    }
  },
  "files": [
    "*.ts",
    "*.d.ts",
    "_esm/**/*.js",
    "_esm/**/*.wasm",
    "_types/**/*.d.ts",
    "_types/**/*.d.ts.map",
    "*.wasm"
  ],
  "homepage": "https://github.com/tadpole-labs/zstd-codec-lib",
  "sideEffects": false,
  "engines": {
    "node": ">=22",
    "npm": ">=8"
  },
  "browserslist": [
    ">0.3%",
    "chrome >= 80",
    "edge >= 80",
    "firefox >= 113",
    "safari >= 16.4",
    "ios_saf >= 16.4",
    "not dead",
    "fully supports wasm-simd",
    "fully supports wasm-bulk-memory",
    "fully supports wasm-signext"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "provenance": true
  }
}
